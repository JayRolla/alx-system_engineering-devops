#!/usr/bin/env bash
# This script displays numbers from 1 to 100
# and prints 'FizzBuzz' for multiples of 3 and 5,
# 'Fizz' for multiples of 3, and 'Buzz' for multiples of 5.

for ((i=1; i<=100; i++))
do
    output=""
    if ((i % 3 == 0))
    then
        output+="Fizz"
    fi
    if ((i % 5 == 0))
    then
        output+="Buzz"
    fi
    if [ -z "$output" ]
    then
        echo "$i"
    else
        echo "$output"
    fi
done
